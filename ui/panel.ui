<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="Panel" parent="GtkBox">
        <property name="orientation">vertical</property>
   

        <child>
            <object class="GtkBox">
                <property name="height-request">90</property>
                <property name="margin-start">10</property>
                <property name="margin-end">10</property>
                <property name="spacing">10</property>

                <child>
                    <object class="GtkImage">
                        <property name="icon-name">printer-symbolic</property>
                        <!-- <property name="file">res/测试设备.png</property> -->
                        <property name="pixel-size">60</property>
                    </object>
                </child>
                
                <child>
                    <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="margin-top">30</property>
                        <property name="margin-bottom">30</property>
                        <property name="spacing">5</property>
                        <property name="hexpand">True</property>
                        <property name="halign">start</property>

                        <child>
                            <object class="GtkLabel">
                                <property name="label">&lt;b&gt;M3-00-355&lt;/b&gt;</property>
                                <property name="use-markup">True</property>
                            </object>
                        </child>

                        <child>
                            <object class="GtkBox">
                                <property name="spacing">5</property>
                                <child>
                                    <object class="GtkImage">
                                        <style><class name="green-dot"></class></style>
                                    </object>
                                </child>

                                <child>
                                    <object class="GtkLabel">
                                        <property name="label">&lt;small&gt;已连接&lt;/small&gt;</property>
                                        <property name="use-markup">True</property>
                                    </object>
                                </child>

                            </object>
                        </child>
                    </object>
                </child>

                <child>
                    <object class="GtkBox">
                        <property name="margin-top">35</property>
                        <property name="margin-bottom">35</property>
                        <property name="spacing">5</property>

                        <child>
                            <object class="GtkButton" id="device_manager">
                                <property name="icon-name">network-transmit-receive-symbolic</property>
                                <signal name="clicked" handler="device_manager_clicked" swapped="no"/>
                            </object>
                        </child>

                        <child>
                            <object class="GtkButton" id="device_discovery">
                                <property name="icon-name">emblem-system-symbolic</property>
                                <signal name="clicked" handler="device_discovery_clicked" swapped="no"/>
                            </object>
                        </child>
                    </object>
                </child>

            </object>
        </child>
        <child>
            <object class="GtkSeparator">
                <property name="orientation">horizontal</property>
            </object>
        </child>
        <child>
            <object class="GtkScrolledWindow">
                <child>
                    <object class="GtkStack" id='stack'>
                        <property name="visible">True</property>
                        <property name="opacity">1</property>

                        <child>
                            <object class="GtkStackPage">
                                <property name="name">总览</property>
                                <property name="child">
                                    <object class="GtkBox">
                                        <property name="orientation">vertical</property>
                                        <child>
                                            <object class="GtkLabel">
                                                <property name="label">参数总览</property>
                                                <property name="halign">start</property>
                                                <property name="margin-top">10</property>
                                                <property name="margin-bottom">10</property>
                                                <property name="margin-start">10</property>
                                                <property name="margin-end">10</property>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="GtkListBox" id="params">
                                                <property name="vexpand">True</property>
                                                <property name="margin-start">10</property>
                                                <property name="margin-end">10</property>
                                                <property name="selection-mode">GTK_SELECTION_NONE</property>
                                                <signal name="row-activated" handler="on_activate_cursor_row" swapped="no"/>
