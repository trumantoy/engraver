<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="Propbar" parent="GtkScrolledWindow">
        <style><class name="propbar"/></style>
        <property name="hscrollbar-policy">never</property>
        <property name="vscrollbar-policy">never</property>
        <property name="max-content-width">600</property>
        <property name="max-content-height">600</property>
        <property name="propagate-natural-width">True</property>

        <child>
            <object class="GtkBox" id="tools">
                <property name="orientation">horizontal</property>
                <property name="spacing">5</property>
                <child>
                    <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="spacing">5</property>

                        <child>
                            <object class="GtkBox">                     
                                <child>
                                    <object class="GtkLabel">
                                        <property name="label">X</property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkSpinButton" id="x">
                                        <property name="digits">2</property>
                                        <property name="adjustment">
                                            <object class="GtkAdjustment">
                                                <property name="lower">-100</property>
                                                <property name="upper">100</property>
                                                <property name="step_increment">1</property>
                                                <property name="page_increment">10</property>
                                                <property name="page_size">0</property>
                                                <property name="value">0</property>
                                            </object>
                                        </property>
                                        <property name="climb_rate">1</property>
                                        <!-- <signal name="value-changed" handler="point_size_value_changed"/> -->
                                    </object>
                                </child>
                            </object>
                        </child>

                        <child>
                            <object class="GtkBox">
                                <child>
                                    <object class="GtkLabel">
                                        <property name="label">Y</property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkSpinButton" id="y">
                                        <property name="digits">2</property>
                                        <property name="adjustment">
                                            <object class="GtkAdjustment">
                                                <property name="lower">-100</property>
                                                <property name="upper">100</property>
                                                <property name="step_increment">1</property>
                                                <property name="page_increment">10</property>
                                                <property name="page_size">0</property>
                                                <property name="value">0</property>
                                            </object>
                                        </property>
                                        <property name="climb_rate">1</property>
                                        <!-- <signal name="value-changed" handler="point_size_value_changed"/> -->
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>
                
                <child>
                    <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="homogeneous">True</property>    
                        <property name="spacing">5</property>
                        <child>
                            <object class="GtkBox">
                                <child>
                                    <object class="GtkLabel">
                                        <property name="label">宽</property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkSpinButton" id="w">
                                        <property name="digits">2</property>
                                        <property name="adjustment">
                                            <object class="GtkAdjustment">
                                                <property name="lower">-100</property>
                                                <property name="upper">100</property>
                                                <property name="step_increment">1</property>
                                                <property name="page_increment">10</property>
                                                <property name="page_size">0</property>
                                                <property name="value">0</property>
                                            </object>
                                        </property>
                                        <property name="climb_rate">1</property>
                                        <!-- <signal name="value-changed" handler="point_size_value_changed"/> -->
                                    </object>
                                </child>
                            </object>
                        </child>

                        <child>
                            <object class="GtkBox">
                                <child>
                                    <object class="GtkLabel">
                                        <property name="label">高</property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkSpinButton" id="h">
                                        <property name="digits">2</property>
                                        <property name="adjustment">
                                            <object class="GtkAdjustment">
                                                <property name="lower">-100</property>
                                                <property name="upper">100</property>
                                                <property name="step_increment">1</property>
                                                <property name="page_increment">10</property>
                                                <property name="page_size">0</property>
                                                <property name="value">0</property>
                                            </object>
                                        </property>
                                        <property name="climb_rate">1</property>
                                        <!-- <signal name="value-changed" handler="point_size_value_changed"/> -->
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>

                <child>
                    <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="homogeneous">True</property>    
                        <property name="spacing">5</property>
                        <child>
                            <object class="GtkBox">
                                <child>
                                    <object class="GtkLabel">
                                        <property name="label">旋转</property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkSpinButton" id="rotate">
                                        <property name="digits">0</property>
                                        <property name="adjustment">
                                            <object class="GtkAdjustment">
                                                <property name="lower">-180</property>
                                                <property name="upper">180</property>
                                                <property name="step_increment">1</property>
                                                <property name="page_increment">10</property>
                                                <property name="page_size">0</property>
                                                <property name="value">0</property>
                                            </object>
                                        </property>
                                        <property name="climb_rate">1</property>
                                        <!-- <signal name="value-changed" handler="point_size_value_changed"/> -->
                                    </object>
                                </child>
                            </object>
                        </child>

                        <child>
                            <object class="GtkBox">
                                <property name="halign">end</property>
                                <child>
                                    <object class="GtkButton" id="remove">
                                        <style><class name="del"/></style>
                                        <property name="label">删除</property>
                                        <property name="icon-name">user-trash-symbolic</property>
                                        <signal name="clicked" handler="btn_remove_clicked" />
                                    </object>
                                </child>
                            </object>
                        </child>

                    </object>
                </child>

                <child>
                    <object class="GtkSeparator">
                        <property name="orientation">vertical</property>
                    </object>
                </child>

                <!-- 属性栏 -->
                <child>
                    <object class="GtkStack" id='stack'>
                        <property name="visible">True</property>
                        <property name="opacity">1</property>

                        <child>
                            <object class="GtkStackPage">
                                <property name="name">Label</property>
                                <property name="child">
                                    <object class="GtkBox">
                                        <property name="orientation">horizontal</property>
                                        <property name="spacing">5</property>
                                        <child>
                                            <object class="GtkBox">
                                                <property name="orientation">vertical</property>
                                                <property name="homogeneous">True</property>    
                                                <property name="spacing">5</property>

                                                <child>
                                                    <object class="GtkFontDialogButton" id="font">
                                                        <property name="dialog">
                                                            <object class="GtkFontDialog"></object>
                                                        </property>
                                                        <!-- <signal name="notify::font-desc" handler="font_features_font_changed" swapped="no"/> -->
                                                    </object>
                                                </child>
                                                <child>
                                                    <object class="GtkEntry" id="text">
                                                        <signal name="activate" handler="text_activate" />
                                                    </object>
                                                </child>
                                            </object>
                                        </child>

                                    </object>
                                </property>

                            </object>
                        </child>
                        <child>
                            <object class="GtkStackPage">
                                <property name="name">Bitmap</property>
                                <property name="child">
                                    <object class="GtkBox">
                                        <property name="orientation">horizontal</property>
                                        <property name="spacing">5</property>
                                    </object>
                                </property>

                            </object>
                        </child>
                        <child>
                            <object class="GtkStackPage">
                                <property name="name">Vector</property>
                                <property name="child">
                                    <object class="GtkBox">
                                        <property name="orientation">horizontal</property>
                                        <property name="spacing">5</property>
                                    </object>
                                </property>

                            </object>
                        </child>
                        <property name="visible-child-name">Label</property>
                    </object>
                </child>

                <child>
                    <object class="GtkSeparator">
                        <property name="orientation">vertical</property>
                    </object>
                </child>
<!-- 
                <child>
                    <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="homogeneous">True</property>    
                        <property name="spacing">5</property>

                        <child>
                            <object class="GtkButton">
                                <property name="label">删除</property>
                                <property name="icon-name">user-trash-symbolic</property>
                            </object>
                        </child>
                        

                        <child>
                            <object class="GtkBox">
                            </object>
                        </child>


                    </object>
                </child> -->

                


            </object>
        </child>
    </template>
</interface>