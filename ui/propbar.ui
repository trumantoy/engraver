<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="Propbar" parent="GtkScrolledWindow">
        <style><class name="propbar"/></style>
        <property name="hscrollbar-policy">never</property>
        <property name="vscrollbar-policy">never</property>
        <property name="max-content-width">600</property>
        <property name="max-content-height">600</property>
        <property name="propagate-natural-width">True</property>

        <child>
            <object class="GtkBox" id="tools">
                <property name="orientation">horizontal</property>
                <property name="spacing">5</property>
                <child>
                    <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="spacing">5</property>

                        <child>
                            <object class="GtkBox">                     
                                <child>
                                    <object class="GtkLabel">
                                        <property name="label">X (mm)</property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkSpinButton">
                                        <property name="digits">2</property>
                                        <property name="adjustment">
                                            <object class="GtkAdjustment">
                                                <property name="lower">-100</property>
                                                <property name="upper">100</property>
                                                <property name="step_increment">1</property>
                                                <property name="page_increment">10</property>
                                                <property name="page_size">0</property>
                                                <property name="value">0</property>
                                            </object>
                                        </property>
                                        <property name="climb_rate">1</property>
                                        <!-- <signal name="value-changed" handler="point_size_value_changed"/> -->
                                    </object>
                                </child>
                            </object>
                        </child>

                        <child>
                            <object class="GtkBox">
                                <child>
                                    <object class="GtkLabel">
                                        <property name="label">Y (mm)</property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkSpinButton">
                                        <property name="digits">2</property>
                                        <property name="adjustment">
                                            <object class="GtkAdjustment">
                                                <property name="lower">-100</property>
                                                <property name="upper">100</property>
                                                <property name="step_increment">1</property>
                                                <property name="page_increment">10</property>
                                                <property name="page_size">0</property>
                                                <property name="value">0</property>
                                            </object>
                                        </property>
                                        <property name="climb_rate">1</property>
                                        <!-- <signal name="value-changed" handler="point_size_value_changed"/> -->
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>
                
                <child>
                    <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="homogeneous">True</property>    
                        <property name="spacing">5</property>
                        <child>
                            <object class="GtkBox">
                                <child>
                                    <object class="GtkLabel">
                                        <property name="label">宽 (mm)</property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkSpinButton">
                                        <property name="digits">2</property>
                                        <property name="adjustment">
                                            <object class="GtkAdjustment">
                                                <property name="lower">-100</property>
                                                <property name="upper">100</property>
                                                <property name="step_increment">1</property>
                                                <property name="page_increment">10</property>
                                                <property name="page_size">0</property>
                                                <property name="value">0</property>
                                            </object>
                                        </property>
                                        <property name="climb_rate">1</property>
                                        <!-- <signal name="value-changed" handler="point_size_value_changed"/> -->
                                    </object>
                                </child>
                            </object>
                        </child>

                         <child>
                            <object class="GtkBox">
                                <child>
                                    <object class="GtkLabel">
                                        <property name="label">高 (mm)</property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkSpinButton">
                                        <property name="digits">2</property>
                                        <property name="adjustment">
                                            <object class="GtkAdjustment">
                                                <property name="lower">-100</property>
                                                <property name="upper">100</property>
                                                <property name="step_increment">1</property>
                                                <property name="page_increment">10</property>
                                                <property name="page_size">0</property>
                                                <property name="value">0</property>
                                            </object>
                                        </property>
                                        <property name="climb_rate">1</property>
                                        <!-- <signal name="value-changed" handler="point_size_value_changed"/> -->
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>

                <child>
                    <object class="GtkBox">
                        <property name="orientation">vertical</property>
                        <property name="homogeneous">True</property>    
                        <property name="spacing">5</property>
                        <child>
                            <object class="GtkBox">
                                <child>
                                    <object class="GtkLabel">
                                        <property name="label">旋转 (°)</property>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkSpinButton">
                                        <property name="digits">0</property>
                                        <property name="adjustment">
                                            <object class="GtkAdjustment">
                                                <property name="lower">-180</property>
                                                <property name="upper">180</property>
                                                <property name="step_increment">1</property>
                                                <property name="page_increment">10</property>
                                                <property name="page_size">0</property>
                                                <property name="value">0</property>
                                            </object>
                                        </property>
                                        <property name="climb_rate">1</property>
                                        <!-- <signal name="value-changed" handler="point_size_value_changed"/> -->
                                    </object>
                                </child>
                            </object>
                        </child>

                        <child>
                            <object class="GtkBox">
                            </object>
                        </child>



                    </object>
                </child>

                

        
                <child>
                    <object class="GtkSeparator">
                        <property name="orientation">vertical</property>
                    </object>
                </child>

                


            </object>
        </child>
    </template>
</interface>